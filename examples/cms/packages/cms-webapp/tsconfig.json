{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "outDir": "./build/server",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"],
  "files": ["src/server/index.ts"]
}