{
  "private": true,
  "name": "email-hook",
  "version": "0.1.0",
  "bin": {
    "email-hook": "bin/email-hook.js"
  },
  "scripts": {
    "build": "tsc -b ./ packages/*",
    "watch": "tsc -b ./ packages/* -w",
