    "deploy-infra": "cdk deploy --app 'node bin/email-hook.js'",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/node": "^8.9.4",
    "aws-cdk": "^0.21.0",
    "lerna": "^3.8.0",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^0.21.0",
    "@aws-cdk/aws-codebuild": "^0.21.0",
    "@aws-cdk/aws-codepipeline": "^0.21.0",
    "@aws-cdk/aws-codepipeline-api": "^0.21.0",
    "@aws-cdk/aws-dynamodb": "^0.21.0",
    "@aws-cdk/aws-s3": "^0.21.0",
    "@aws-cdk/aws-ses": "^0.21.0",
    "@aws-cdk/aws-sns": "^0.21.0",
    "@aws-cdk/cdk": "^0.21.0"
  },
  "workspaces": [
    "packages/*"
  ]
