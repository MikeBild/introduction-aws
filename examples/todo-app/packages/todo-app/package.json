{
  "private": true,
  "name": "todo-app",
  "version": "1.0.0",
  "description": "Todo App",
  "main": "index.js",
  "scripts": {
    "build": "rimraf -r dist .cache && parcel build src/server/index.tsx --target node -d dist/server && parcel build src/browser/index.tsx -d dist/browser",
    "dev": "dotenv -- node src/server/index.ts",
    "start": "dotenv -- node dist/server",
    "deploy": "cdk deploy"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "cors": "^2.8.5",
    "dotenv-cli": "^1.4.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^0.21.0",
    "@aws-cdk/aws-dynamodb": "^0.21.0",
    "@aws-cdk/aws-lambda": "^0.21.0",
    "@aws-cdk/cdk": "^0.21.0",
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "@types/aws-serverless-express": "^3.3.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-jss": "^8.6.0",
    "@types/react-router-dom": "^4.3.1",
    "apollo-boost": "^0.1.23",
    "apollo-client": "^2.4.8",
    "graphql": "^14.0.2",
    "helmet": "^3.15.0",
    "parcel": "^1.11.0",
    "react": "16.7.0-alpha.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "typeface-roboto": "^0.0.54",
    "typescript": "^3.2.2"
  }
}
