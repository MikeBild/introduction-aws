{
  "private": true,
  "name": "todo-app",
  "version": "1.0.0",
  "description": "Todo App",
  "main": "index.js",
  "scripts": {
    "build": "rimraf -rf ./dist ./.cache && dotenv -- parcel build src/index.html --public-url ./",
    "dev": "dotenv -- parcel serve src/index.html",
    "start": "dotenv -- http-server ./dist",
    "deploy": "cdk deploy"
  },
  "dependencies": {
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^0.21.0",
    "@aws-cdk/aws-dynamodb": "^0.21.0",
    "@aws-cdk/aws-lambda": "^0.21.0",
    "@aws-cdk/cdk": "^0.21.0",
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "apollo-boost": "^0.1.23",
    "apollo-client": "^2.4.8",
    "dotenv-cli": "^1.4.0",
    "graphql": "^14.0.2",
    "helmet": "^3.15.0",
    "parcel": "^1.11.0",
    "react": "16.7.0-alpha.0",
    "react-apollo": "^2.3.3",
    "react-dom": "16.7.0-alpha.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "typeface-roboto": "^0.0.54",
    "typescript": "^3.2.2"
  }
