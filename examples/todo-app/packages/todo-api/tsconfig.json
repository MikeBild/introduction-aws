    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2016",
      "es2015"
