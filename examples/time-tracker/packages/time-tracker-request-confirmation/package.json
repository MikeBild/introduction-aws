{
  "private": true,
  "name": "time-tracker-request-confirmation",
  "version": "1.0.0",
  "description": "Time Tracking Request and Confirmation",
  "main": "dist/index.js",
  "dependencies": {
    "aws-sdk": "^2.382.0",
    "dotenv-cli": "^1.4.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-iam": "^0.21.0",
    "@aws-cdk/aws-lambda": "^0.21.0",
    "@aws-cdk/aws-stepfunctions": "^0.21.0",
    "@types/node": "^10.12.18",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "clear": "rimraf -rf dist",
    "build": "tsc",
    "start": "dotenv -- node dist/index.js",
    "dev": "dotenv -- ts-node-dev src/index.ts",
    "aws:bootstrap": "cdk bootstrap",
    "aws:deploy": "cdk deploy",
    "deploy": "npm run clear && npm run build && cp -R node_modules ./dist && npm run aws:bootstrap && npm run aws:deploy"
  }
}
