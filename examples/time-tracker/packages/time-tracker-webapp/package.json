{
  "private": true,
  "name": "time-tracker-webapp",
  "version": "1.0.1",
  "description": "Time Tracking App",
  "main": "index.js",
  "scripts": {
    "build": "rimraf -rf ./dist ./.cache && dotenv -- parcel build src/index.html --public-url ./",
    "dev": "dotenv -- parcel serve src/index.html",
    "start": "dotenv -- http-server ./dist",
    "deploy": "cdk deploy"
  },
  "dependencies": {
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-s3": "^0.20.0",
    "@aws-cdk/aws-s3-deployment": "^0.20.0",
    "@aws-cdk/cdk": "^0.20.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "apollo-boost": "^0.1.23",
    "dotenv-cli": "^1.4.0",
    "graphql": "^14.0.2",
    "parcel": "^1.11.0",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.2.2"
  }
}
